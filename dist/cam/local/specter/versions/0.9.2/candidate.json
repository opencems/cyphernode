{
  "version": "0.9.2",
  "dependencies": [
    {
      "label": "bitcoin",
      "version": "~0.20.0"
    }
  ],
  "files": {
    "docker-compose.yaml": "docker-compose.yaml"
  },
  "isExposed":true,
  "port": 25441,
  "availableRoles": [
    {
      "name": "user",
      "description": "Standard user, can spend and receive",
      "autoAssign": false
    }
  ],
  "accessPolicies": [
    {
      "patterns": [".+"],
      "roles": ["*"],
      "actions": ["options"],
      "effect": "allow"
    },
    {
      "patterns": ["favicon.ico$"],
      "roles": ["*"],
      "actions": ["get"],
      "effect": "allow"
    },
    {
      "patterns": [".+"],
      "roles": ["user"],
      "actions": ["*"],
      "effect": "allow"
    }
  ]
}
